{
    "manifest_version": 3,
    "version": "1.0",
    "name": "ElevenLabs Voice Agent",
    "host_permissions": [
        "https://*.elevenlabs.io/*",
        "ws://*.elevenlabs.io/*",
        "https://extensions.webfu.se/*",
        "https://api.iconify.design/*",
        "https://api.unisvg.com/*",
        "https://api.simplesvg.com/*"
    ],
    "content_scripts": [
        {
            "js": [
                "content.js"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "env": [
        {
            "key": "AGENT_KEY",
            "value": "agent_6701k9cs5v41f5e8g8sn7xctykpf"
        }
    ]
}